<?xml version="1.0" encoding="UTF-8"?>
<!-- Example -->
<ComplexObservableProperty xmlns="http://schemas.opengis.net/op"
    xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.opengis.net/op ../schemas/op.xsd" gml:id="ID003">
    <label>2m air temperature</label>
    <basePhenomenon xlink:href="http://areference.com/air_temperature"/>
    <constraint>
        <swe:Quantity definition="http://areference.com/height">            
            <swe:label>2m Height</swe:label>
            <swe:uom xlink:href="http://someref.com/m"/>
            <swe:value>2</swe:value>
        </swe:Quantity>
    </constraint>
</ComplexObservableProperty>
