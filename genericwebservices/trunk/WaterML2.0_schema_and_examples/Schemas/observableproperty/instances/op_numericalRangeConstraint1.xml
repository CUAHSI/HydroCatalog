<ComplexObservableProperty xmlns="http://schemas.opengis.net/op"
    xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.opengis.net/op ../schemas/op.xsd" gml:id="ID002">
    <label>Near Infra Red Radiance</label>
    <basePhenomenon xlink:href="http://sweet.jpl.nasa.gov/2.1/propEnergyFlux.owl#Radiance"/>
    <constraint>
        <swe:QuantityRange definition="http://areference.com/wavelength">            
            <swe:label>Near Infra Red</swe:label>
            <swe:uom xlink:href="http://someref.com/m"/>
            <swe:value>780 3000</swe:value>
        </swe:QuantityRange>
    </constraint>
</ComplexObservableProperty>
