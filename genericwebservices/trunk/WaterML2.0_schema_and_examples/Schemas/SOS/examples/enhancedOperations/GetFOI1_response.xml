<?xml version="1.0" encoding="UTF-8"?>
<!--=====================================================================
Example Response for GetFeatureOfInterest operation which contains features which have an observableProperty as specified in the request GetFOI1.xml and are observed by the procedure whose ID has been passed in the request.
=======================================================================-->
<sos:GetFeatureOfInterestResponse xmlns="http://www.opengis.net/sos/2.0"  xmlns:sos="http://www.opengis.net/sos/2.0" xmlns:fes="http://www.opengis.net/fes/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:swes="http://www.opengis.net/swes/2.0" xmlns:sams="http://www.opengis.net/samplingSpatial/2.0"
	xmlns:sf="http://www.opengis.net/sampling/2.0"
	 xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/sos/2.0
http://schemas.opengis.net/sos/2.0.0/sos.xsd http://www.opengis.net/samplingSpatial/2.0 http://schemas.opengis.net/samplingSpatial/2.0/spatialSamplingFeature.xsd">
	<featureMember>
	<sams:SF_SpatialSamplingFeature
	gml:id="st1">
	<gml:identifier codeSpace="">http://www.my_namespace.org/fois/st1</gml:identifier>
	<sf:type
		xlink:href="http://www.opengis.net/def/samplingFeatureType/OGC-OM/2.0/SF_SamplingPoint"/>
	<sf:sampledFeature
		xlink:href="http://wfs.example.org?request=getFeature&amp;featureid=Rhine_Sandbank_123"/>
	<sams:shape>
		<gml:Point
			gml:id="st1p">
			<gml:pos
				srsName="http://www.opengis.net/def/crs/EPSG/0/4326">50.7167 7.76667</gml:pos>
		</gml:Point>
	</sams:shape>
	</sams:SF_SpatialSamplingFeature>
	</featureMember>
</sos:GetFeatureOfInterestResponse>
